    "name": "zed-frontend",
    "private": true,
    "version": "2.1.0",
    "type": "module",
    "scripts": {
        "dev:server": "tsx server/index.ts",
        "dev:client": "vite",
        "dev": "concurrently \"npm:server\" \"npm:web\"",
        "server": "tsx server/index.ts",
        "server:bootstrap": "tsx server/scripts/bootstrap.ts",
        "web": "npm run dev --prefix zed-web",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
