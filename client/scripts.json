{
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "dev:backend": "NODE_ENV=development tsx server/index.ts",
    "dev:simple": "NODE_ENV=development tsx server/index.simple.ts",
    "dev:reliable": "bash start-reliable.sh",
    "frontend": "vite --host 0.0.0.0 --port 5173",
    "backend": "NODE_ENV=development tsx server/index.simple.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "clean": "rm -rf dist node_modules/.cache .vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest"
  }
}