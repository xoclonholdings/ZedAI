{
  "compilerOptions": {
    "target": "ESNext",
  "module": "Node16",
    "moduleResolution": "node16",
  "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src"],
  "types": ["react", "react-dom"]
}
